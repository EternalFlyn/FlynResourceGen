{
  "multipart": [
    {
      "when": { "tier": "1" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_block_t1" }
    }, {
      "when": { "tier": "2" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_block_t2" }
    }, {
      "when": { "tier": "3" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_block_t3" }
    }, {
      "when": { "tier": "4" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_block_t4" }
    }, {
      "when": { "tier": "5" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_block_t5" }
    }, {
      "when": { "fluid": "water" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_fluid_water" }
    }, {
      "when": { "fluid": "mixed" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_fluid_mixed" }
    }, {
      "when": { "fluid": "lava" },
      "apply": { "model": "flyn_resource_gen:block/resource_gen_fluid_lava" }
    }
  ]
}